<p id="notice"><%= notice %></p>

<h1>Aaaas</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aaaas.each do |aaaa| %>
      <tr>
        <td><%= aaaa.name %></td>
        <td><%= link_to 'Show', aaaa %></td>
        <td><%= link_to 'Edit', edit_aaaa_path(aaaa) %></td>
        <td><%= link_to 'Destroy', aaaa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aaaa', new_aaaa_path %>
